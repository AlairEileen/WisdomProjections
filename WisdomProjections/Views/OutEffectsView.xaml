<Grid x:Class="WisdomProjections.Views.OutEffectsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WisdomProjections.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Border x:Name="gContainer">
        <Grid>
            <Image x:Name="img"/>
            <MediaElement x:Name="video" Visibility="Hidden" LoadedBehavior="Manual" UnloadedBehavior="Manual" MediaEnded="Video_MediaEnded" Loaded="Video_Loaded"/>
            <Canvas x:Name="canvas"/>
        </Grid>
    </Border>
</Grid>
